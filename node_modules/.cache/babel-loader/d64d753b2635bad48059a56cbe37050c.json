{"ast":null,"code":"import { ref } from \"vue\";\nimport { useAuthStore } from \"@/stores/userAuth\";\nimport axios from \"axios\";\nexport default {\n  __name: 'TheLogin',\n\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const authStore = useAuthStore();\n    console.log(authStore);\n    const username = ref(\"\");\n    const password = ref(\"\");\n    const showPassword = ref(false);\n    const passwordType = ref(\"password\");\n    let success = ref(false);\n\n    function toggleShow() {\n      showPassword.value = !showPassword.value;\n\n      if (showPassword.value) {\n        passwordType.value = \"text\";\n      } else {\n        passwordType.value = \"password\";\n      }\n    }\n\n    function submitAuthDetails() {\n      const params = new URLSearchParams();\n      params.append(\"username\", username.value);\n      params.append(\"password\", password.value);\n      const headers = {\n        Accept: \"application/json\",\n        \"Content-Type\": \"application/x-www-form-urlencoded\"\n      };\n      axios.post(\"http://localhost:8000/login\", params, {\n        headers: headers\n      }).then(response => {\n        success.value = true;\n        console.log(response.data[\"access_token\"]);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n    const __returned__ = {\n      authStore,\n      username,\n      password,\n      showPassword,\n      passwordType,\n      success,\n      toggleShow,\n      submitAuthDetails,\n      ref,\n      useAuthStore,\n      axios\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AA6CA,SAASA,GAAT,QAAoB,KAApB;AACA,SAASC,YAAT,QAA6B,mBAA7B;AACA,OAAOC,KAAP,MAAkB,OAAlB;;;;;;;;IAEA,MAAMC,SAAS,GAAGF,YAAY,EAA9B;IACAG,OAAO,CAACC,GAAR,CAAYF,SAAZ;IACA,MAAMG,QAAQ,GAAGN,GAAG,CAAC,EAAD,CAApB;IACA,MAAMO,QAAQ,GAAGP,GAAG,CAAC,EAAD,CAApB;IACA,MAAMQ,YAAY,GAAG